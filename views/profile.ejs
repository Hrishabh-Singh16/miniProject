    <!DOCTYPE html>
    <html lang="en">
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>Document</title>
        
        <!-- bootstrap -->
        <!-- <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css"> -->

        <!-- css -->
        <link rel="stylesheet" href="/css/profile.css">
        <link rel="stylesheet" href="https://unpkg.com/boxicons@latest/css/boxicons.min.css">

        <!-- Google Fonts -->
        <link rel="preconnect" href="https://fonts.googleapis.com">
        <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
        <link href="https://fonts.googleapis.com/css2?family=Poppins:wght@300;400;500;600;700;800;900&display=swap" rel="stylesheet">
    </head>
    <body>

        <section class="login-con">
            <div class="nav-bar"> 
                    <ul class="nav-list" >
                        <a href="#"><li class="account">My Account</li></a>
                        <a href="#"><li class="home"><i class='bx bxs-home' ></i>Home</li></a>
                        <a href="#"><li class="offer"><i class='bx bxs-offer'></i>Offer</li></a>
                        <a href="#"><li class="search"><i class='bx bx-search-alt-2'></i>Search</li></a>
                        <a href="#"><li class="cart"><i class='bx bxs-cart'></i>Cart</li></a>
                    </ul>
            </div>
                <div class="user-name">
                        <div class="nav-list1">
                            <div class="info-column">
                                <ul class="info-item">
                                    <a href=""><span class="name">Souradip Roy</span></a>
                                    <a href=""><span class="contact"><i class='bx bxs-phone'></i> 8822370121</span></a>
                                </ul>
                            </div>
                            <div class="edit-section">
                                <a href=""><span class="edit"><i class='bx bx-edit'></i> Edit Profile</span></a>
                            </div>
                        </div>
                </div>

                <div class="main-con">
                    <div class="nav-bar2">
                        <div class="nav-con">
                            <ul class="con-list">
                                <a href="/profile"><li class="order active">Orders</li></a>
                                <a href="/favourites"><li class="favourites">Favourites</li></a>
                                <a href="#"><li class="payments">Payments</li></a>
                            </ul>
                        </div>
                        <div class="con-list1">
                            <div class="heading">
                                <h2>Past Orders<h2>
                            </div> 
                            <div class="food-list">
                                <% for(let i=0; i<=5; i++){ %>
                                    <div class="food-card">
                                        <div class="container">
                                            <div class="image-container">
                                                <img src="/css/img/pizza.avif">
                                            </div>
                                            <div class="restaurant-info">
                                                <div class="restaurant-name">Chicken Momo x 1</div>
                                                <div class="restaurant-address">Guwahati</div>
                                                <div class="order-details">ORDER #11101</div>
                                                <button class="view-details">VIEW DETAILS</button>
                                            </div>
                                            <div class="item-info">
                                                <div class="delivered">
                                                    <div class="delivered-status">Delivered</div>
                                                    <div class="delivered-icon">✔</div>
                                                </div>                                        
                                                <div class="order-summary">
                                                    <div class="order-rating"><i class="bx bxs-star"></i>4.5(10)</div>
                                                    <div class="total-paid">Total Paid: ₹50</div>
                                                </div>    
                                            </div>
                                          
                                            <div class="btn">
                                                <button class="reorder-button">REORDER</button>
                                                <button class="help-button">HELP</button>
                                            </div>
                                        </div>
                                    </div>
                                <% } %>
                            </div>      
                        </div>
                    </div>
                </div>
        </section>
        
    </body>

