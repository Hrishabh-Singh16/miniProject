<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Dashboard</title>

    <!-- bootstrap -->
    <!-- <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css"> -->

    <!-- css -->
    <link rel="stylesheet" href="/css/dashboard.css">
    <link rel="stylesheet" href="https://unpkg.com/boxicons@latest/css/boxicons.min.css">

    <!-- Google Fonts -->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Poppins:wght@300;400;500;600;700;800;900&display=swap" rel="stylesheet">
</head>
<body>
    <section class="main-con">
        <div class="nav-bar">
            <div class="con">
                <div class="heading">
                    <h2>The Grand Ã‰clat</h2>
                </div>
                <div class="nav-con">
                    <ul class="nav-list">
                        <a href="/dashboard"><li class="dashboard"><i class='bx bxs-dashboard'></i>Dashboard</li></a>
                        <a href="/dashboard/orders"><li class="order active"><i class='bx bx-restaurant'></i>Orders</li></a>
                        <a href="/dashboard/customers"><li class="customer"><i class='bx bxs-user' ></i>Customers</li></a>
                        <a href="#"><li class="emoloyee"><i class='bx bxs-user' ></i>Employees</li></a>
                        <a href="#"><li class="menu"><i class='bx bxs-food-menu' ></i>Menu</li></a>
                        <a href="#"><li class="table"><i class='bx bxs-card' ></i>Table</li></a>
                    </ul>
                </div>
            </div>
        </div>
    
        <div class="dashboard">
            <div class="nav-bar2">
                <a href=""><i class='bx bx-menu' ></i></a>
                <div class="nav-icons">
                    <p>Hello, Ranadip</p>
                    <i class='bx bxs-user-circle'></i>
                </div>
            </div>
            <div class="order-dash">
                <div class="dash-titile">
                    <h2>Orders List</h2>
                    <p>Dashboard / Orders</p>
                </div>
                <div class="o-one">
                    <table>
                        <tr>
                          <th>Order id</th>
                          <th>Date</th>
                          <th>Customer</th>
                          <th>Contact Number</th>
                          <th>Amount</th>
                          <th>Order Status</th>
                        </tr>
                        <tr>
                          <td>#12345</td>
                          <td>21 June 2024, 12:32 pm</td>
                          <td>Ranadip</td>
                          <td>9957416364</td>
                          <td>299.00</td>
                          <td>Delivered</td>
                          <td class="btn-td">
                            <a href="" class="o-btn"><i class='bx bx-data'></i></a>
                            <a href="" class="o-btn"><i class='bx bx-edit'></i></a>
                        </td>
                        </tr>
                        <% for(let i=1; i<20 ; i++){ %>
                            <tr>
                                <td>#12346</td>
                                <td>21 June 2024, 12:32 pm</td>
                                <td>Tony</td>
                                <td>9957416364</td>
                                <td>499.00</td>
                                <td>Pending</td>
                                <td class="btn-td">
                                    <a href="" class="o-btn"><i class='bx bx-data'></i></a>
                                    <a href="" class="o-btn"><i class='bx bx-edit'></i></a>
                                </td>
                            </tr>
                        <% } %>
                      </table>
                </div>
            </div>

        </div>
    </section>
    <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
    <script src="/js/graph.js"></script>
</body>
</html>